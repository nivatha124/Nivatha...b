  button.modernize.hidden-tablet, .button.modernize.hidden-tablet { display: none; }
  button.modernize.hidden-desktop, .button.modernize.hidden-desktop { display: flex; }
  button.modernize.visible-desktop, .button.modernize.visible-desktop { display: none; }
}

